query listContributions {
 rateLimit{
  cost
  remaining
  resetAt
 }
 search(query:"created:>2008-04-10", type:USER, first:20, after:"Y3Vyc29yOjEw"){
  userCount
  pageInfo {
  	endCursor
  }
  edges{
   node{
    ... on User{
      id
      name
	  repositoriesContributedTo(includeUserRepositories:true first:20 orderBy:{direction:DESC field:STARGAZERS}) {
        totalCount
        nodes{
          stargazers{
            totalCount
          }
          primaryLanguage{
            name
          }
          id
          name
        }
      }
    }
  }
  }
}
}
